{
  "pages": [
    {
      "path": "pages/index",
      "style": {
        "navigationBarTitleText": "首页"
      }
    }
  ],
  "subPackages": [
    {
      "root": "pages-sub",
      "pages": [
        {
          "path": "login/index",
          "style": {
            "navigationBarTitleText": "登录",
            "mp-weixin": {
              "usingComponents": {
                "loginform": "plugin://myPlugin/loginform"
              }
            }
          }
        },
        {
          "path": "ai-ceyan/index",
          "style": {
            "navigationBarTitleText": "AI测颈纹",
            "navigationStyle": "custom"
          }
        },
        {
          "path": "ai-ceyan/uploaded",
          "style": {
            "navigationBarTitleText": "上传成功海报",
            "navigationStyle": "custom"
          }
        }
      ],
      "plugins": {
        "myPlugin": {
          "version": "1.0.25",
          "provider": "wx49ba8c2e81bce02f"
        }
      }
    },
    {
      "root": "pages/mine",
      "pages": [
        {
          "path": "address/list",
          "style": {
            "navigationBarTitleText": "我的地址"
          }
        },
        {
          "path": "address/manage",
          "style": {
            "navigationBarTitleText": "编辑地址"
          }
        }
      ]
    }
  ],
  "sitemapLocation": "sitemap.json",
  "globalStyle": {
    "pageOrientation": "portrait",
    "backgroundTextStyle": "light",
    "navigationBarBackgroundColor": "#ffffff",
    "navigationBarTitleText": "IMEIK 爱+",
    "navigationBarTextStyle": "black",
    "usingComponents": {
      "t-captcha": "plugin://captcha/t-captcha"
    }
  },
  "insetLoader": {
    "config": {
      "toast": "<x-toast ref='toast'></x-toast>",
      "captcha": "<global-captcha ref='captcha'></global-captcha>",
      "privacyPopup": "<global-privacy-popup ref='privacyComponent'></global-privacy-popup>"
    },
    "label": [
      "login",
      "captcha",
      "privacyPopup",
      "toast"
    ],
    "rootEle": ".*"
  }
}